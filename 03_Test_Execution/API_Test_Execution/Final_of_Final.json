		{
			"name": "API_TC_001 Get all products",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Test Case 1: Kiểm tra Status Code phải là 200 OK\r",
							"pm.test(\"API_TC_001 Status code is 200 OK\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Test Case 2: Kiểm tra dữ liệu trả về phải đúng định dạng JSON\r",
							"pm.test(\"Response is in JSON format\", function () {\r",
							"    pm.expect(pm.response.json()).to.be.an('object');\r",
							"});\r",
							"\r",
							"// Test Case 3: Kiểm tra thời gian phản hồi API dưới 3s\r",
							"pm.test(\"Response time is acceptable (<3000ms)\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(3000);\r",
							"});\r",
							"\r",
							"// Test Case 4: Kiểm tra Response Body có chứa danh sách sản phẩm (products array)\r",
							"pm.test(\"Response body contains 'products' array\", function () {\r",
							"    const body = pm.response.json();\r",
							"    pm.expect(body.products).to.be.an('array');\r",
							"    pm.expect(body.products.length).to.be.above(0); // Kiểm tra array không rỗng\r",
							"});\r",
							"\r",
							"pm.test(\"Verify Product have id, name, price\" , function(){\r",
							"    const firstproduct = pm.response.json().products[0];\r",
							"    pm.expect(firstproduct).to.have.property('id');\r",
							"    pm.expect(firstproduct).to.have.property('name');\r",
							"    pm.expect(firstproduct).to.have.property('price');\r",
							"});"
						],
